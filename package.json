{
  "name": "x-satori",
  "type": "module",
  "version": "0.0.3",
  "description": "use Vue file to generate Open Graph SVG or PNG by satori",
  "author": "Q.Ben Zheng <zhengqbbb@gmail.com> (https://github.com/Zhengqbbb/)",
  "license": "MIT",
  "homepage": "https://github.com/Zhengqbbb/x-satori#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zhengqbbb/x-satori"
  },
  "bugs": "https://github.com/Zhengqbbb/x-satori/issues",
  "keywords": [
    "cli"
  ],
  "exports": {
    "./*": "./*",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./vue": {
      "types": "./dist/vue.d.ts",
      "import": "./dist/vue.mjs"
    },
    "./astro": {
      "types": "./dist/astro.d.ts",
      "import": "./dist/astro.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "bin": {
    "x-satori": "bin/x-satori.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild && cpx src/yoga.wasm dist/shared",
    "start": "x-satori",
    "cz": "czg",
    "lint": "eslint .",
    "prepare": "simple-git-hooks && cpx node_modules/yoga-wasm-web/dist/yoga.wasm src",
    "prepack": "npm run build",
    "release": "run-s release:bump release:publish",
    "release:bump": "bumpp -y -c 'build: :bookmark: publish v%s'",
    "release:publish": "pnpm publish",
    "demo:vue:svg": "pnpm -C examples/vue to:svg",
    "demo:vue:png": "pnpm -C examples/vue to:png"
  },
  "dependencies": {
    "satori": "^0.4.2",
    "satori-html": "^0.3.2",
    "yoga-wasm-web": "^0.3.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.3",
    "@rollup/plugin-replace": "^5.0.2",
    "@types/node": "^18.15.3",
    "bumpp": "^9.0.0",
    "cpx2": "^4.2.2",
    "czg": "^1.6.0",
    "eslint": "^8.36.0",
    "lint-staged": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "pathe": "^1.1.0",
    "pnpm": "^7.29.1",
    "simple-git-hooks": "^2.8.1",
    "tsx": "^3.12.5",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.2",
    "vite": "^4.1.4",
    "vue": "^3.2.47",
    "x-satori": "workspace:*"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged && pnpm lint"
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  }
}
