{
  "name": "playground-x-satori",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "description": "use Vue file to generate Open Graph SVG or PNG by satori. Run script or use CLI",
  "scripts": {
    "dev:og": "  x-satori -t ./Template.vue -c ./config.ts --dev",
    "build:og": "x-satori -t ./Template.vue -c ./config.ts -o image.svg",
    "gen:svg": "tsx gen-svg.mts",
    "gen:png": "tsx gen-png.mts"
  },
  "dependencies": {
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@resvg/resvg-js": "^2.6.2",
    "@types/node": "^24.1.0",
    "tsx": "^4.20.3",
    "vite": "^7.0.6",
    "x-satori": "^0.3.0"
  }
}
